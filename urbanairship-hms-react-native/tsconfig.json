{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["es2015"],
    "moduleResolution": "node",
    "strict": true,
    "target": "es5",
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "include": ["js/**/*.ts", "js/**/*.tsx"],
  "exclude": ["node_modules", "js/__tests__"]
}
