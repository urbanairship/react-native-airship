    "acorn": "^7.1.1",
    "acorn-jsx": "^5.0.2",
    "eslint": "^5.16.0",
    "jest": "^24.9.0",
    "jetifier": "^1.6.4",
    "react": "16.10.1",
    "react-native": "0.61.5",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.17.7",
