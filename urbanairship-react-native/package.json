    "react-component",
    "ios",
    "android",
    "airship"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.64.0",
    "@types/react-test-renderer": "^17.0.1",
    "@types/prop-types": "^15.7.3",
    "@react-native-community/bob": "^0.16.2",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "ts-jest": "^26.0.1",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urbanairship/react-native-module.git"
  },
  "license": "Apache-2.0",
  "@react-native-community/bob": {
    "source": "js",
    "output": "lib",
    "targets": [
      "commonjs",
      "typescript"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "modulePathIgnorePatterns": [
      "lib/"
    ]
  },
  "scripts": {
    "prepare": "bob build",
    "test": "jest --verbose",
    "generate-docs": "typedoc --out docs --exclude **/index.ts"
